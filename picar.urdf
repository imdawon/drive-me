<?xml version="1.0"?>
<robot name="picar_x">

  <material name="silver">     <color rgba="0.75 0.75 0.75 1.0"/> </material>
  <material name="dark_grey">  <color rgba="0.2 0.2 0.2 1.0"/>    </material>
  <material name="white">      <color rgba="0.9 0.9 0.9 1.0"/>    </material>
  <material name="black">      <color rgba="0.0 0.0 0.0 1.0"/>    </material>

  <link name="base_link">
    <visual>
      <origin xyz="0 0 0.01" rpy="0 0 0"/>
      <geometry> <box size="0.18 0.10 0.003"/> </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.01" rpy="0 0 0"/>
      <geometry> <box size="0.18 0.10 0.003"/> </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>

    <visual>
      <origin xyz="-0.02 0 0.025" rpy="0 0 0"/>
      <geometry> <box size="0.085 0.06 0.015"/> </geometry>
      <material name="dark_grey"/>
    </visual>
    
    <visual>
        <origin xyz="0.08 0 0.04" rpy="0 0 0"/>
        <geometry> <box size="0.01 0.03 0.03"/> </geometry>
        <material name="silver"/>
    </visual>
  </link>

  <link name="fl_steering_knuckle">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
      <geometry> <cylinder length="0.01" radius="0.005"/> </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="fl_steering_joint" type="revolute">
    <parent link="base_link"/>
    <child link="fl_steering_knuckle"/>
    <origin xyz="0.07 0.06 0.0" rpy="0 0 0"/> 
    <axis xyz="0 0 1"/>
    <limit lower="-0.78" upper="0.78" effort="5.0" velocity="10.0"/>
  </joint>

  <link name="fl_wheel">
    <inertial>
      <mass value="0.05"/>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry> <cylinder length="0.025" radius="0.0325"/> </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry> <cylinder length="0.025" radius="0.0325"/> </geometry>
    </collision>
  </link>
  <joint name="fl_wheel_joint" type="continuous">
    <parent link="fl_steering_knuckle"/>
    <child link="fl_wheel"/>
    <origin xyz="0 0.02 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.01" friction="0.01"/>
  </joint>

  <link name="fr_steering_knuckle">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
      <geometry> <cylinder length="0.01" radius="0.005"/> </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="fr_steering_joint" type="revolute">
    <parent link="base_link"/>
    <child link="fr_steering_knuckle"/>
    <origin xyz="0.07 -0.06 0.0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.78" upper="0.78" effort="5.0" velocity="10.0"/>
  </joint>

  <link name="fr_wheel">
    <inertial>
      <mass value="0.05"/>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry> <cylinder length="0.025" radius="0.0325"/> </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry> <cylinder length="0.025" radius="0.0325"/> </geometry>
    </collision>
  </link>
  <joint name="fr_wheel_joint" type="continuous">
    <parent link="fr_steering_knuckle"/>
    <child link="fr_wheel"/>
    <origin xyz="0 -0.02 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.01" friction="0.01"/>
  </joint>

  <link name="rl_wheel">
    <inertial>
      <mass value="0.05"/>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry> <cylinder length="0.025" radius="0.0325"/> </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry> <cylinder length="0.025" radius="0.0325"/> </geometry>
    </collision>
  </link>
  <joint name="rl_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rl_wheel"/>
    <origin xyz="-0.07 0.08 0" rpy="0 0 0"/> 
    <axis xyz="0 1 0"/>
    <limit effort="1.0" velocity="20.0"/>
  </joint>

  <link name="rr_wheel">
    <inertial>
      <mass value="0.05"/>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry> <cylinder length="0.025" radius="0.0325"/> </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry> <cylinder length="0.025" radius="0.0325"/> </geometry>
    </collision>
  </link>
  <joint name="rr_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rr_wheel"/>
    <origin xyz="-0.07 -0.08 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="1.0" velocity="20.0"/>
  </joint>

</robot>